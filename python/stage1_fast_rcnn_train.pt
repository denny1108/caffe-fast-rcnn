    }
  }
}
layer {
  name: "mode_score"
  type: "Softmax"
  bottom: "fc6_sel"
  top: "mode_score"
}
layer {
  name: "mode_score_slice"
  type: "Slice"
  bottom: "mode_score"
  top: "mode1_score"
  top: "mode2_score"
  slice_param {
    axis: 1
    slice_point: 1
  }
}
layer {
  name: "mode1_weight"
  type: "Concat"
  bottom: "mode1_score"
  bottom: "mode1_score"
  top: "mode1_weight"
