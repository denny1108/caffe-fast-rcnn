    axis: 1
  }
}
layer {
  name: "mode2_weight"
  type: "Concat"
  bottom: "mode2_score"
  bottom: "mode2_score"
  top: "mode2_weight"
  concat_param {
    axis: 1
  }
}
layer {
  name: "weighted_prob1"
  type: "Eltwise"
  bottom: "cls_prob"
  bottom: "mode1_weight"
  top: "weighted_prob1"
  eltwise_param { operation: PROD }
}
layer {
  name: "weighted_prob2"
  type: "Eltwise"
  bottom: "cls_prob_p"
  bottom: "mode2_weight"
  top: "weighted_prob2"
  eltwise_param { operation: PROD }
}
layer {
  name: "cls_prob_fuse"
  type: "Eltwise"
  bottom: "weighted_prob1"
  bottom: "weighted_prob2"
  top: "cls_prob_fuse"
  eltwise_param { operation: SUM }
}
layer {
  name: "loss_cls"
  type: "MultinomialLogisticLoss"
  bottom: "cls_prob_fuse"
  bottom: "labels"
  top: "loss_cls"
  loss_weight: 1
}
layer {
  name: "silence_bbox_pred"
  type: "Silence"
  bottom: "bbox_pred"
}
layer {
  name: "silence_bbox_pred_p"
